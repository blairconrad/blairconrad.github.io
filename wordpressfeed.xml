---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0"
     xmlns:content="http://purl.org/rss/1.0/modules/content/"
     xmlns:wfw="http://wellformedweb.org/CommentAPI/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:atom="http://www.w3.org/2005/Atom"
     xmlns:sy="http://purl.org/rss/1.0/modules/syndication/"
     xmlns:slash="http://purl.org/rss/1.0/modules/slash/"
     xmlns:georss="http://www.georss.org/georss" xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#" xmlns:media="http://search.yahoo.com/mrss/">
  <channel>
    <title>{{ site.title }}</title>
    <atom:link href="{{ site.feed_url }}" rel="self" type="application/rss+xml" />
    <link>{{ site.url }}</link>
    <description>{{ site.title }} - {{ site.description }}</description>
    <lastBuildDate>Sun, 24 Nov 2013 05:33:45 +0000</lastBuildDate>
    <language>en</language>
    <image>
      <url>http://www.gravatar.com/avatar/{{ site.email | remove: ' ' | strip_newlines | downcase | md5 }}</url>
      <title>{{ site.title }} Feed</title>
      <link>{{ site.url }}</link>
    </image>
    {% for post in site.posts limit:10 %}
    <item>
      <title>{{ post.title }}</title>
      <link>{{ site.url }}{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <dc:creator><![CDATA[{{ post.author }}]]></dc:creator>
      <!--<category><![CDATA[Development]]></category>-->
      <guid>{{ site.url }}{{ post.id }}</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}
  </channel>
</rss>
